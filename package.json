    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.3",
        "babel-preset-airbnb": "^3.2.0",
        "babel-preset-env": "^1.7.0",
        "eslint": "^5.15.3",
        "eslint-config-airbnb": "^17.1.0",
        "eslint-plugin-import": "^2.16.0",
        "eslint-plugin-jsx-a11y": "^6.2.1",
        "eslint-plugin-react": "^7.12.4",
        "husky": "^1.3.1",
        "lint-staged": "^8.1.5",
        "mocha": "6.0.2",
        "supertest": "4.0.0"
    }
