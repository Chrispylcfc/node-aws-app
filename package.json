{
    "name": "node-aws-app",
    "description": "Node.js Application for AWS",
    "version": "1.0.0",
    "private": true,
    "author": "Christopher Powell",
    "scripts": {
        "start": "babel-node index.js",
        "validate": "npx eslint *.js --ext *.jsx",
